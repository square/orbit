{
  "name": "@square/orbit",
  "version": "0.1.0",
  "description": "Square eCommerce's (formerly Weebly) Vue.js UI component library",
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.config.dev.js --open",
    "dev-build": "NODE_ENV=production webpack --config build/webpack.config.build.js --watch",
    "analyze-build": "ANALYZE=true NODE_ENV=production webpack --config build/webpack.config.build.js",
    "build": "NODE_ENV=production webpack --config build/webpack.config.build.js",
    "build:clean": "rm $(build/release/outputFiles.js)",
    "lint": "npm run lint:js",
    "lint:js": "eslint --format=friendly --ext js,vue,md --cache src",
    "lint:css": "stylelint './src/**/*.{css,vue}' --cache",
    "release": "np --github",
    "version": "./build/release/version.sh",
    "postpublish": "./build/release/postpublish.sh",
    "pretest": "npx i-peers -a ci",
    "test": "karma start ./testing/karma.config.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{vue,js}": "eslint --format=friendly",
    "src/**/*.{vue,css}": "stylelint"
  },
  "files": [
    "utils",
    "components",
    "*.{css,css.map}"
  ],
  "os": [
    "darwin",
    "linux"
  ],
  "devEngines": {
    "node": "^12.15.0",
    "npm": "^6.13.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/square/orbit.git"
  },
  "keywords": [
    "orbit",
    "vue",
    "components",
    "design system",
    "square",
    "weebly",
    "component library",
    "ui components"
  ],
  "license": "Apache-2.0",
  "style": "index.css",
  "dependencies": {
    "cleave.js": "1.4.10",
    "intersection-observer": "^0.5.1",
    "lodash": "^4.17.10",
    "popper.js": "^1.15.0",
    "resize-observer-polyfill": "^1.5.0",
    "semver": "^5.7.0",
    "stickybits": "^3.6.5",
    "stickyfilljs": "^2.1.0",
    "tinycolor2": "^1.4.1",
    "vue-proxi": "^1.0.0",
    "vue-pseudo-window": "^0.1.0",
    "vue-slicksort": "^0.1.10",
    "vue-subslot": "^1.2.2",
    "vue-vnode-syringe": "^2.0.1",
    "@square/orbit-icons": "^1.0.0-rc.1"
  },
  "peerDependencies": {
    "axios": "^0.17.1",
    "vue": "~2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.6.2",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "cache-loader": "^2.0.1",
    "code-inject-loader": "0.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-markdown": "github:privatenumber/eslint-plugin-markdown",
    "eslint-plugin-vue": "^6.2.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "htmlvue-loader": "^1.1.1",
    "husky": "^4.2.5",
    "jasmine-core": "^3.3.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-private-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "lint-staged": "^10.2.11",
    "md-vue-loader": "^2.0.0",
    "np": "github:privatenumber/np#4.0.2-orbit",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss": "^7.0.27",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-calc": "^7.0.2",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-properties": "^9.1.1",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-import": "^12.0.1",
    "postcss-input-range": "^4.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "postcss-reporter": "^6.0.0",
    "style-loader": "^0.19.1",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.1.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^15.9.1",
    "vue-router": "^3.1.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "~2.6.12",
    "webpack": "^3.3.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-dev-server": "^3.1.11",
    "webpack-distsize": "^0.1.0",
    "webpack-merge": "^4.1.4"
  }
}
